  "main": "server.js",
  "scripts": {
    "start": "npm run dev",
    "build": "npm-run-all clean transpile",
    "server:dev": "babel-node ./src/bin/www",
    "server:prod": "node ./dist/bin/www",
    "dev": "NODE_ENV=development & npm-run-all build server:dev",
